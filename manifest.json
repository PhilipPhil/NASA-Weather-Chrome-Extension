{
  "manifest_version": 2,
  "name": "Nasa APOD New Tab",
  "description": "Changes new Tab",
  "version": "1.0.3",
  "icons": {
    "16": "/img/icon/icon16.png",
    "48": "/img/icon/icon48.png",
    "128": "/img/icon/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "geolocation",
    "topSites"
  ],
  "web_accessible_resources": [
    "img/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  }
}